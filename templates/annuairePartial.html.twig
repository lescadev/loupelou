{% if response is defined%}
    <div class="row pre-scrollable">
    {%for result in response  %}
            <div class="card border-dark ml-1" style="width: 10rem;">
                <div class="card-header">{{result.nom}}</div>
                <div class="card-body text-dark">
                    <p class="card-text">Adresse: {{ result.adresse }}</p>
                </div>
            </div>
    {%endfor%}
    </div>
{% endif %}