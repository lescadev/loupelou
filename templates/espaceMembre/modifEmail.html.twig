{% extends 'base.html.twig' %}
{% block stylesheets %}<link rel="stylesheet" href="/css/stylesProfil.css">{% endblock %}
{% block body %}
<div id="containerProfil">
{% include "espaceMembre/menuModif.html.twig" %}
<div id="blockProfil">
    <h2 id="profilTitle">Modifier mon adresse mail</h1>
    <input class="blockInput" disabled value="{{ mail }}" required type="text">
    {{form_start(form)}}
        {{ form_row(form.email) }}
        <button type="submit">Valider</button>
    {{ form_end(form, {'render_rest': false}) }}
        
</div>
</div>
{% endblock %}